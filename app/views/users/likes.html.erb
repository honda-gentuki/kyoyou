<div class="main">
  <div class="user-main">
    <div class="inner">
      <div class="user__wrapper">
        <h2 class="page-heading">
          <%= "#{@user.nickname}先生がいいねした投稿"%>
        </h2>
        <div class="card__wrapper">
          <%= render partial: 'posts/post', collection: @user.posts %>
        </div>
        <div class="paginate">
          <%= paginate @posts %>
        </div>
      </div>
    </div>
  </div>
</div>